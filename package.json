  "keywords": [
    "protobuf",
    "static",
    "grpc",
    "pb"
  ],
  "author": {
    "name": "Edvard Chen",
    "email": "pigeon73101@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "byline": "^5.0.0",
    "debug": "^4.3.1",
    "globby": "^11.0.3",
    "grpc-tools": "^1.10.0",
    "lodash": "^4.17.21",
    "tmp": "^0.2.1",
    "ts-protoc-gen": "^0.14.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/byline": "^4.2.32",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.35",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.3",
    "jest": "26",
